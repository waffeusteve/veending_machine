		<relativePath>../vending-parent/pom.xml</relativePath>
	</parent>
	<artifactId>vending-webservices</artifactId>
	<name>vending-webservices</name>
	<packaging>jar</packaging>
	<dependencies>
		<dependency>
			<groupId>org.pkfrc.vending</groupId>
			<artifactId>vending-dto</artifactId>
			<version>${vending.version}</version>
		</dependency>
		<dependency>
			<groupId>org.pkfrc.vending</groupId>
			<artifactId>vending-services</artifactId>
			<version>${vending.version}</version>
		</dependency>
		<dependency>
