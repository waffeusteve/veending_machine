		</dependency>
		<dependency>
			<groupId>org.pkfrc.core</groupId>
			<artifactId>core-webservices</artifactId>
			<version>${pkfrc-core.version}</version>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-actuator</artifactId>
		</dependency>
		<dependency>
			<groupId>io.micrometer</groupId>
